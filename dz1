#include <iostream>
#include <cmath> 

int main()
{
    double a;
    double b;
    double c;
    double x1;
    double x2;
    std::cout << "Input a: ";
    std::cin >> a;
    std::cout << "Input b: ";
    std::cin >> b;
    std::cout << "Input c: ";
    std::cin >> c;
    if((b*b - 4*a*c) >= 0)
    {
        x1 = ( -1*b + sqrt(b*b - 4*a*c) ) / (2 * a);
        x2 = ( -1*b - sqrt(b*b - 4*a*c) ) / (2 * a);
        std::cout << "Solution:(" << x1 << ";" << x2 << ")";       
    }
    else
    {
        std::cout << "No solution\n";
    }
    return 0;
}
